a = float(input())
b = float(input())
c = str(input())
if c == '+':
  print(a + b)
elif c == '-':
  print (a - b)
elif c == '*':
  print(a * b)
elif c == 'pow':
  print(a ** b)
elif (b == 0 and c == '/') or (b == 0 and c == 'mod') or (b == 0 and c == 'div'):
  print('Деление на 0!')
elif c == '/':
  print(a / b)
elif c == 'mod':
  print(a % b)
elif c == 'div':
  print(a // b)
